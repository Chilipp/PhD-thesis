@Article{Lewis1998,
  author    = {Lewis, J. M.},
  journal   = {Bulletin of the American Meteorological Society},
  title     = {Clarifying the Dynamics of the General Circulation: Phillips{\textquotesingle}s 1956 Experiment},
  year      = {1998},
  month     = {jan},
  number    = {1},
  pages     = {39--60},
  volume    = {79},
  abstract  = {In the mid-1950s, amid heated debate over the physical mechanisms that controlled the known features of the atmosphere's general circulation, Norman Phillips simulated hemispheric motion on the high-speed computer at the Institute for Advanced Study. A simple energetically consistent model was integrated for a simulated time of approximately 1 month. Analysis of the model results clarified the respective roles of the synoptic-scale eddies (cyclones-anticyclones) and mean meridional circulation in the maintenance of the upper-level westerlies and the surface wind regimes. Furthermore, the modeled cyclones clearly linked surface frontogenesis with the upper-level Charney-Eady wave. In addition to discussing the model results in light of the controversy and ferment that surrounded general circulation theory in the 1940s-1950s, an effort is made to follow Phillips's scientific path to the experiment.},
  doi       = {10.1175/1520-0477(1998)079<0039:ctdotg>2.0.co;2},
  publisher = {American Meteorological Society},
  url       = {https://ui.adsabs.harvard.edu/abs/1998BAMS...79...39L},
}


@Article{DurbanGlasbey2001,
  author   = {M. Durban and C.A. Glasbey},
  title    = {Weather modelling using a multivariate latent Gaussian model},
  journal  = {Agric. For. Meteorol.},
  year     = {2001},
  volume   = {109},
  number   = {3},
  pages    = {187 - 201},
  issn     = {0168-1923},
  abstract = {We propose a vector auto-regressive moving average process as a model for daily weather data. For the rainfall variable a monotonic transformation is applied to achieve marginal normality, thus, defining a latent variable, with zero rainfall data corresponding to censored values below a threshold. Methodology is presented for model identification, estimation and validation, illustrated using data from Mylnefield, Scotland. The new model, a vector second-order auto-regressive first-order moving average (VARMA(2,1)) process, fits the data better, and produces more realistic simulated series than, existing models of Richardson [Water Resources Res. 17 (1981) 182] and Peiris and McNicol [Agric. Forest Meteorol. 79 (1996) 219].},
  doi      = {http://dx.doi.org/10.1016/S0168-1923(01)00268-4},
  keywords = {Auto-correlation},
  url      = {http://www.sciencedirect.com/science/article/pii/S0168192301002684},
}


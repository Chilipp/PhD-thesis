@Article{ChuineRegniere2017,
  author   = {Chuine, Isabelle and Régnière, Jacques},
  title    = {Process-Based Models of Phenology for Plants and Animals},
  journal  = {Annual Review of Ecology, Evolution, and Systematics},
  year     = {2017},
  volume   = {48},
  number   = {1},
  pages    = {159-182},
  abstract = { Phenology is a key aspect of plant and animal life strategies that determines the ability to capture seasonally variable resources. It defines the season and duration of growth and reproduction and paces ecological interactions and ecosystem functions. Phenology models have become a key component of models in agronomy, forestry, ecology, and biogeosciences. Plant and animal process-based phenology models have taken different paths that have so far not crossed. Yet, they share many features because plant and animal annual cycles also share many characteristics, from their stepwise progression, including a resting period, to their dependence on similar environmental factors. We review the strengths and shortcomings of these models and the divergences in modeling approaches for plants and animals, which are mostly due to specificities of the questions they tackle. Finally, we discuss the most promising avenues and the challenges phenology modeling needs to address in the upcoming years. },
  doi      = {10.1146/annurev-ecolsys-110316-022706},
  eprint   = {https://doi.org/10.1146/annurev-ecolsys-110316-022706},
  url      = {https://doi.org/10.1146/annurev-ecolsys-110316-022706},
}

